@model MVC5.Models.ApplicationUser

<h2>Edit User</h2>


@using (Html.BeginForm())
{
  @Html.AntiForgeryToken()

  <div class="form-horizontal">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">
      @Html.LabelFor(model => model.IsEnabled, htmlAttributes: new { @class = "col-md-2" })
      <div class="">
        @Html.EditorFor(model => model.IsEnabled)
        @Html.ValidationMessageFor(model => model, "", new { @class = "text-danger" })
      </div>
    </div>

    <div class="form-group">
      @Html.LabelFor(model => model.UserName, htmlAttributes: new { @class = "col-md-2" })
      @Html.DisplayFor(modelItem => Model.UserName, new { htmlAttributes = new { @class = "form-control" } })
    </div>

    <div class="form-group">
      <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Save" class="btn btn-default" />
      </div>
    </div>
  </div>
}

<div>
  @Html.ActionLink("Back to List", "Index")
</div>