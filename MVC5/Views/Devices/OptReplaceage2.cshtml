@model MVC5.Models.Device

@{
  ViewBag.Title = "title";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

@*header*@
<div class="row">
  <div class="col-sm-4">
    <h2>最优更换年龄</h2>

  </div>
  <div>
    <input type="button" class="btn btn-primary pull-right" value="回生命管理选单"
           onclick="location.href='@Url.Action("LifeManageMenu", "Devices", new { id = Model.ID })'" />
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-heading">最优更换年龄</div>
  <div class="panel-body">
    <div class="center-block text-center">
      <h3>
        <label>根据以上提前/延遲的年份 @Model.Name 的最佳更換年齡為 @ViewBag.Cost 年</label>
      </h3>
      <p></p>
      <img src="~/users/@User.Identity.Name/@Model.ID/@ViewBag.name?@DateTime.Now" alt="defereffect.jpg" />
      <p></p>
      <h3>
        <label>Monte Carlo 仿真 (Proactive + Reactive)</label>
      </h3>
      <p></p>
      <img src="~/users/@User.Identity.Name/@Model.ID/@ViewBag.name1?@DateTime.Now" alt="montecarloage.jpg" />
      <p></p>
    </div>
  </div>
</div>
